{
    "name": "multiverse/notazzsdk",
    "description": "Notazz SDK api para integração com Notazz",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Tumadjian",
            "email": "tumadjian@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Multiverse\\" : "src/Multiverse"
        }
    },
    "require": {
        "php": "^7.2.0 || ^8.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "test": "phpunit tests",
        "check-standards": "phpcs --colors --standard=PSR12 src",
        "fix-standards": "phpcbf --colors --standard=PSR12 src"
    }
}
